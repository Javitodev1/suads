---
import {
  AsideLayout,
  Creativity,
  AdMethod,
  Hero,
  SalesMethod,
  AdProduction,
  PlaceholderIcon,
} from '@/components'

export interface HomeLayoutSlots {
  'hero-header': string
  'hero-body': string
  'hero-cta': string
  'creativity-header': string
  'creativity-body': string
  'creativity-card-icon-1': string
  'creativity-card-header-1': string
  'creativity-card-body-1': string
  'creativity-card-icon-2': string
  'creativity-card-header-2': string
  'creativity-card-body-2': string
  'creativity-card-icon-3': string
  'creativity-card-header-3': string
  'creativity-card-body-3': string
  'ad-method-header': string
  'ad-method-body': string
  'ad-method-card-header-1': string
  'ad-method-card-body-1': string
  'ad-method-card-header-2': string
  'ad-method-card-body-2': string
  'ad-method-card-header-3': string
  'ad-method-card-body-3': string
  'ad-method-card-header-4': string
  'ad-method-card-body-4': string
  'ad-method-card-header-5': string
  'ad-method-card-body-5': string
  'ad-method-cta': string
  'sales-method-header': string
  'sales-method-card-header-1': string
  'sales-method-card-body-1': string
  'sales-method-card-header-2': string
  'sales-method-card-body-2': string
  'sales-method-card-header-3': string
  'sales-method-card-body-3': string
  'sales-method-card-header-4': string
  'sales-method-card-body-4': string
  'sales-method-cta': string
  'sales-method-body': string
  'ad-production-header': string
  'ad-production-card-header-1': string
  'ad-production-card-body-1': string
  'ad-production-card-header-2': string
  'ad-production-card-body-2': string
  'ad-production-card-header-3': string
  'ad-production-card-body-3': string
  'ad-production-cta': string
}

interface Props {
  title: string
  heroVideoSrc: string
}

const { title, heroVideoSrc } = Astro.props
---

<AsideLayout title={title}>
  <Hero src={heroVideoSrc}>
    <slot
      name='hero-header'
      slot='header'>
      No layout slot hero-header
    </slot>
    <slot
      name='hero-body'
      slot='body'>
      No layout slot hero-body
    </slot>
    <slot
      name='hero-cta'
      slot='cta'>
      No layout slot hero-cta
    </slot>
  </Hero>

  <Creativity>
    <slot
      name='creativity-header'
      slot='header'>
      No layout slot creativity-header
    </slot>
    <slot
      name='creativity-body'
      slot='body'>
      No layout slot creativity-body
    </slot>
    <slot
      name='creativity-card-icon-1'
      slot='card-icon-1'>
      <PlaceholderIcon variant={'dark'} />
    </slot>
    <slot
      name='creativity-card-header-1'
      slot='card-header-1'>
      No layout slot creativity-card-header- 1
    </slot>
    <slot
      name='creativity-card-body-1'
      slot='card-body-1'>
      No layout slot creativity-card-body- 1
    </slot>
    <slot
      name='creativity-card-icon-2'
      slot='card-icon-2'>
      <PlaceholderIcon variant={'dark'} />
    </slot>
    <slot
      name='creativity-card-header-2'
      slot='card-header-2'>
      No layout slot creativity-card-header- 2
    </slot>
    <slot
      name='creativity-card-body-2'
      slot='card-body-2'>
      No layout slot creativity-card-body- 2
    </slot>
    <slot
      name='creativity-card-icon-3'
      slot='card-icon-3'>
      <PlaceholderIcon variant={'dark'} />
    </slot>
    <slot
      name='creativity-card-header-3'
      slot='card-header-3'>
      No layout slot creativity-card-header- 3
    </slot>
    <slot
      name='creativity-card-body-3'
      slot='card-body-3'>
      No layout slot creativity-card-body- 3
    </slot>
  </Creativity>

  <AdMethod>
    <slot
      name='ad-method-header'
      slot='header'>
      No layout slot ad-method-header
    </slot>
    <slot
      name='ad-method-body'
      slot='body'>
      No layout slot ad-method-body
    </slot>
    <slot
      name='ad-method-card-header-1'
      slot='card-header-1'>
      No layout slot ad-method-card-header-1
    </slot>
    <slot
      name='ad-method-card-body-1'
      slot='card-body-1'>
      No layout slot ad-method-card-body-1
    </slot>
    <slot
      name='ad-method-card-header-2'
      slot='card-header-2'>
      No layout slot ad-method-card-header-2
    </slot>
    <slot
      name='ad-method-card-body-2'
      slot='card-body-2'>
      No layout slot ad-method-card-body-2
    </slot>
    <slot
      name='ad-method-card-header-3'
      slot='card-header-3'>
      No layout slot ad-method-card-header-3
    </slot>
    <slot
      name='ad-method-card-body-3'
      slot='card-body-3'>
      No layout slot ad-method-card-body-3
    </slot>
    <slot
      name='ad-method-card-header-4'
      slot='card-header-4'>
      No layout slot ad-method-card-header-4
    </slot>
    <slot
      name='ad-method-card-body-4'
      slot='card-body-4'>
      No layout slot ad-method-card-body-4
    </slot>
    <slot
      name='ad-method-card-header-5'
      slot='card-header-5'>
      No layout slot ad-method-card-header-5
    </slot>
    <slot
      name='ad-method-card-body-5'
      slot='card-body-5'>
      No layout slot ad-method-card-body-5
    </slot>
    <slot
      name='ad-method-cta'
      slot='cta'>
      No layout slot ad-method-cta
    </slot>
  </AdMethod>

  <SalesMethod>
    <slot
      name='sales-method-header'
      slot='header'>
      No layout slot sales-method-header
    </slot>
    <slot
      name='sales-method-card-header-1'
      slot='card-header-1'>
      No layout slot sales-method-card-header-1
    </slot>
    <slot
      name='sales-method-card-body-1'
      slot='card-body-1'>
      No layout slot sales-method-card-body-1
    </slot>
    <slot
      name='sales-method-card-header-2'
      slot='card-header-2'>
      No layout slot sales-method-card-header-2
    </slot>
    <slot
      name='sales-method-card-body-2'
      slot='card-body-2'>
      No layout slot sales-method-card-body-2
    </slot>
    <slot
      name='sales-method-card-header-3'
      slot='card-header-3'>
      No layout slot sales-method-card-header-3
    </slot>
    <slot
      name='sales-method-card-body-3'
      slot='card-body-3'>
      No layout slot sales-method-card-body-3
    </slot>
    <slot
      name='sales-method-card-header-4'
      slot='card-header-4'>
      No layout slot sales-method-card-header-4
    </slot>
    <slot
      name='sales-method-card-body-4'
      slot='card-body-4'>
      No layout slot sales-method-card-body-4
    </slot>
    <slot
      name='sales-method-cta'
      slot='cta'>
      No layout slot sales-method-cta
    </slot>
    <slot
      name='sales-method-body'
      slot='body'>
      No layout slot sales-method-body
    </slot>
  </SalesMethod>

  <AdProduction>
    <slot
      name='ad-production-header'
      slot='header'>
      No layout slot ad-production-header
    </slot>
    <slot
      name='ad-production-card-header-1'
      slot='card-header-1'>
      No layout slot ad-production-card-header-1
    </slot>
    <slot
      name='ad-production-card-body-1'
      slot='card-body-1'>
      No layout slot ad-production-card-body-1
    </slot>
    <slot
      name='ad-production-card-header-2'
      slot='card-header-2'>
      No layout slot ad-production-card-header-2
    </slot>
    <slot
      name='ad-production-card-body-2'
      slot='card-body-2'>
      No layout slot ad-production-card-body-2
    </slot>
    <slot
      name='ad-production-card-header-3'
      slot='card-header-3'>
      No layout slot ad-production-card-header-3
    </slot>
    <slot
      name='ad-production-card-body-3'
      slot='card-body-3'>
      No layout slot ad-production-card-body-3
    </slot>
    <slot
      name='ad-production-cta'
      slot='cta'>
      No layout slot ad-production-cta
    </slot>
  </AdProduction>
</AsideLayout>
